{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## 📝 Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["maintenance", "chore", "dependencies"]
    },
    {
      "title": "## 🔒 Security",
      "labels": ["security"]
    },
    {
      "title": "## ⚡ Performance",
      "labels": ["performance"]
    }
  ],
  "ignore_labels": ["ignore", "skip-changelog"],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "No changes in this release",
  "transformers": [],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": []
}
